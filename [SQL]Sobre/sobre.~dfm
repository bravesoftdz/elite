    Top = 80
  end
  object qryVersao: TIBQuery
    Database = dmoPrincipal.Database
    Transaction = traVersao
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'SELECT'
      '  MAX(NUMERO) AS NUMERO,'
      '  MAX(DATA) AS DATA'
      'FROM'
      '  SCRIPTVER')
    UniDirectional = True
    Left = 416
    Top = 128
  end
