  object qryDetalhes: TIBQuery
    Database = dmoPrincipal.Database
    Transaction = traDetalhes
    AfterOpen = qryDetalhesAfterOpen
    BufferChunks = 1000
    CachedUpdates = True
    SQL.Strings = (
      'SELECT *'
      'FROM NATUOPER'
      'WHERE ( CODIGO = :CODIGO )')
    UniDirectional = True
    UpdateObject = updDetalhes
    Left = 324
    Top = 129
    ParamData = <
      item
        DataType = ftString
        Name = 'CODIGO'
        ParamType = ptUnknown
      end>
  end
  object updDetalhes: TIBUpdateSQL
    RefreshSQL.Strings = (
      'Select '
      '  CODIGO,'
      '  NATUREZA,'
      '  CODCENTRO,'
      '  VENDA,'
      '  ESTOQUE,'
      '  ICMS,'
      '  IPI'
      'from NATUOPER '
      'where'
      '  CODIGO = :CODIGO')
    ModifySQL.Strings = (
      'update NATUOPER'
      'set'
      '  CODIGO = :CODIGO,'
      '  NATUREZA = :NATUREZA,'
      '  CODCENTRO = :CODCENTRO,'
      '  VENDA = :VENDA,'
      '  ESTOQUE = :ESTOQUE,'
      '  ICMS = :ICMS,'
      '  IPI = :IPI'
      'where'
      '  CODIGO = :OLD_CODIGO')
    InsertSQL.Strings = (
      'insert into NATUOPER'
      '  (CODIGO, NATUREZA, CODCENTRO, VENDA, ESTOQUE, ICMS, IPI)'
      'values'
      
        '  (:CODIGO, :NATUREZA, :CODCENTRO, :VENDA, :ESTOQUE, :ICMS, :IPI' +
        ')')
    DeleteSQL.Strings = (
      'delete from NATUOPER'
      'where'
      '  CODIGO = :OLD_CODIGO')
    Left = 324
    Top = 241
  end
