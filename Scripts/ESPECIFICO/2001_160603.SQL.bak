UPDATE RDB$RELATION_FIELDS SET
RDB$FIELD_SOURCE = 'INTEIRO',
RDB$NULL_FLAG = NULL
WHERE (RDB$FIELD_NAME = 'OSTIPO') AND
(RDB$RELATION_NAME = 'MOVIMENTOS')!

DELETE FROM MOVIMENTOS WHERE OSTIPO IS NULL!

