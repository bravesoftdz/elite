RDB$FIELD_SOURCE = 'INTEIRO_VALIDO',
RDB$NULL_FLAG = 1
WHERE (RDB$FIELD_NAME = 'OSTIPO') AND
(RDB$RELATION_NAME = 'MOVIMENTOS')!

